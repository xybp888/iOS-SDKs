    objc-ivars:           [ SSAnnotationRenderer._mainScreen,
                            SSAnnotationRenderer._queuedMessage,
                            SSAnnotationRenderer._uiClient,
                            SSAnnotationRenderer.assistanceState,
                            SSAnnotationUser._displayName,
                            SSAnnotationUser._userID,
                            SSConnectionOptions._authMethod,
                            SSConnectionOptions._connectingToIOSdevice,
                            SSConnectionOptions._controlType,
                            SSConnectionOptions._deviceID,
                            SSConnectionOptions._displayName,
                            SSConnectionOptions._hints,
                            SSConnectionOptions._idsDestination,
                            SSConnectionOptions._maxSize,
                            SSConnectionOptions._minimumEncryptionLevel,
                            SSConnectionOptions._noReconnect,
                            SSConnectionOptions._openInFullScreen,
                            SSConnectionOptions._panningMode,
                            SSConnectionOptions._restoreContentFrame,
                            SSConnectionOptions._restoreWindowFrame,
                            SSConnectionOptions._screenQualityMode,
                            SSConnectionOptions._selectedScreen,
                            SSConnectionOptions._sessionSelectionAction,
                            SSConnectionOptions._shouldCurtainScreen,
                            SSConnectionOptions._shouldFallbackToObserve,
                            SSConnectionOptions._shouldGetUserInfo,
                            SSConnectionOptions._shouldReturnToAddressBox,
                            SSConnectionOptions._shouldScaleScreen,
                            SSConnectionOptions._shouldSharePasteboard,
                            SSConnectionOptions._showConnectionProgress,
                            SSConnectionOptions._skipAddressPresentation,
                            SSConnectionOptions._skipUserPassDialogIfPossible,
                            SSConnectionOptions._videoEncodings,
                            SSPanningScrollView._bottomPanActive,
                            SSPanningScrollView._leftPanActive,
                            SSPanningScrollView._rightPanActive,
                            SSPanningScrollView._topPanActive,
                            SSPanningScrollView.bottomRect,
                            SSPanningScrollView.lastPoint,
                            SSPanningScrollView.leftRect,
                            SSPanningScrollView.mDisablePanningEvents,
                            SSPanningScrollView.mPanningMode,
                            SSPanningScrollView.mPanningTimer,
                            SSPanningScrollView.mShowDisconnectedBlurring,
                            SSPanningScrollView.rightRect,
                            SSPanningScrollView.topRect ]
